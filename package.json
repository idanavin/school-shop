{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\"",
    "server": "npx tsx watch server/index.ts",
    "start": "NODE_ENV=production npx tsx server/index.ts",
    "build": "node scripts/set-provider.js postgresql && npx prisma generate && run-p type-check \"build-only {@}\" --",
    "prisma:seed": "npx tsx prisma/seed.ts"
  },
  "preview": "vite preview",
  "build-only": "vite build",
  "type-check": "vue-tsc --build",
  "lint": "eslint . --fix --cache",
  "format": "prettier --write --experimental-cli src/",
  "dependencies": {
    "@prisma/client": "^6.19.1",
    "body-parser": "^2.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "pinia": "^3.0.4",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "~10.6.2",
    "jiti": "^2.6.1",
    "nodemon": "^3.1.11",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.7.4",
    "prisma": "^6.19.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.2.1"
  }
}
